#!/bin/bash

chmod +x ./glog

ps cax | grep "procesoSisOp.sh" > /dev/null

if [ $? -eq 0 ]; 
then
	pkill "procesoSisOp.sh"
	echo "============ Se invoca el comando correctamente. Se aborta la ejecuci贸n de procesoSisOp.sh ============"
	./glog "stop" "Se invoca el comando correctamente. Se aborta la ejecuci贸n de procesoSisOp.sh"
else
	echo "============ [ERROR] No se puede invocar el comando ya que procesoSisOp.sh no se encuentra en ejecuci贸n ============"
	./glog "stop" "No se pudo invocar el comando ya que procesoSisOp.sh no se encuentra en ejecuci贸n" "ERROR"
fi
